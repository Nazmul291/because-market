<script>
  let scrolling = false
  let actionButtonInHeaderDesktop = false
  let actionButtonInHeaderMobile = false
  const desktopActionPosition = 600
  const mobileActionPosition = 1200
  let checking = false
  $(() => {
    $(window).on('scroll load',function(){
      if (checking) {
        return
      }
      checking = true
      const $headerSection = $(".header__section")
      const scrollTop = $(window).scrollTop()
      if (scrollTop > 70 && !scrolling) {
        scrolling = true
        $headerSection.addClass('header__section_scrolling')
      }
      if (scrollTop === 0 && scrolling) {
        scrolling = false
        $(".header__section").removeClass('header__section_scrolling')
      }

      if (scrollTop > desktopActionPosition && !actionButtonInHeaderDesktop) {
        actionButtonInHeaderDesktop = true
        $headerSection.addClass('header__section_action_desktop')
      }
      if (scrollTop <= desktopActionPosition && actionButtonInHeaderDesktop) {
        actionButtonInHeaderDesktop = false
        $(".header__section").removeClass('header__section_action_desktop')
      }

      if (scrollTop > mobileActionPosition && !actionButtonInHeaderMobile) {
        actionButtonInHeaderMobile = true
        $headerSection.addClass('header__section_action_mobile')
      }
      if (scrollTop <= mobileActionPosition && actionButtonInHeaderMobile) {
        actionButtonInHeaderMobile = false
        $(".header__section").removeClass('header__section_action_mobile')
      }

      checking = false
    })
  })

</script>

